{
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/tw",
      "destination": "https://twitter.com/pheralb_"
    },
    {
      "source": "/x",
      "destination": "https://twitter.com/pheralb_"
    },
    {
      "source": "/tv",
      "destination": "https://www.twitch.tv/pheralb"
    },
    {
      "source": "/ln",
      "destination": "https://www.linkedin.com/in/pheralb/"
    },
    {
      "source": "/bs",
      "destination": "https://bsky.app/profile/pheralb.dev"
    },
    {
      "source": "/gh",
      "destination": "https://github.com/pheralb"
    },
    {
      "source": "/git",
      "destination": "https://github.com/pheralb/pheralb.dev"
    }
  ]
}
